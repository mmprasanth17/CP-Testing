package io.cal;

public class CalculatorProject {
	public int add(int a, int b) {
		return (a+b);
	}
	
	public int sub(int x,int y)
	{
		return (x-y);
	}
	
	public int div(int x,int y)
	{
		return (x/y);
	}
	public int mul(int x,int y)
	{
		return (x*y);
	}
	
    public double AreaOfCircle() {
    	return Math.PI*r*r;
    }
}
